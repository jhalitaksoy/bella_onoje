{
  ".grow": {
    "&-relative": {
      "height": null,
      "width": null,
      "flexProps": {
        "flexGrow": 1
      }
    },
    "&-absolute": {
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "height": null,
      "width": null,
      "left": 0,
      "right": 0,
      "bottom": 0,
      "top": 0
    }
  },
  ".padding": {
    "&-vertical": {
      "paddingBottom": 15,
      "paddingTop": 15
    },
    "&-horizontal": {
      "paddingLeft": 15,
      "paddingRight": 15
    }
  },
  ".margin": {
    "&-vertical": {
      "marginBottom": 15,
      "marginTop": 15
    },
    "&-horizontal": {
      "marginLeft": 15,
      "marginRight": 15
    }
  },
  ".shadow": {
    "ios": {
      "shadowOffset": {
        "x": 2,
        "y": 3
      },
      "shadowRadius": 1,
      "shadowOpacity": 0.4,
      "masksToBounds": false
    },
    "android": {
      "elevation": 5
    }
  },
  ".hidden": {
    "width": 0,
    "height": 0,
    "marginBottom": 0,
    "marginTop": 0,
    "marginLeft": 0,
    "marginRight": 0,
    "padding": 0,
    "paddingBottom": 0,
    "paddingLeft": 0,
    "paddingRight": 0,
    "paddingTop": 0
  },
  ".absolute": {
    ".topleft": {
      "height": null,
      "left": 0,
      "top": 0
    },
    ".topright": {
      "height": null,
      "top": 0,
      "right": 0
    },
    ".bottomleft": {
      "height": null,
      "bottom": 0,
      "left": 0
    },
    ".bottomright": {
      "height": null,
      "bottom": 0,
      "right": 0
    },
    "flexProps": {
      "positionType": "ABSOLUTE"
    }
  },
  ".centerChild": {
    "flexProps": {
      "justifyContent": "CENTER"
    }
  },
  ".hide": {
    "visible": false
  },
  "#hello": {
    "backgroundColor": "rgba( 255, 75, 58, 1 )",
    "&-statusbar": {
      "style": "DEFAULT"
    },
    "&-title": {
      "textColor": "rgba( 255, 255, 255, 1 )",
      "font": {
        "name": "",
        "family": "Default",
        "style": "Bold",
        "size": 64,
        "bold": true,
        "italic": false
      },
      "height": 80,
      "backgroundColor": "rgba( 63, 77, 216, 0 )"
    }
  },
  "#header": {
    "&-circle": {
      "width": 73,
      "height": 73,
      "left": 50,
      "top": null,
      "backgroundColor": "rgba( 255, 252, 252, 1 )",
      "&-icon": {
        "image": "bella_onoje.png",
        "width": 60,
        "height": 60,
        "imageFillType": "ASPECTFIT",
        "backgroundColor": "rgba( 0, 0, 0, 0 )",
        "margin": null
      },
      "flexProps": {
        "justifyContent": "CENTER",
        "alignContent": "STRETCH",
        "alignItems": "CENTER"
      },
      "borderRadius": 100,
      "padding": null,
      "&-wrapper": {
        "height": 80,
        "top": 20
      }
    }
  },
  "#wrapper": {
    "&-title": {
      "paddingLeft": 50,
      "height": 180,
      "backgroundColor": "rgba( 68, 206, 95, 0 )"
    },
    "&-headerbar": {
      "&-icon": {
        "flexProps": {
          "alignItems": "CENTER",
          "justifyContent": "SPACE_BETWEEN"
        },
        "borderRadius": 20,
        "backgroundColor": "rgba( 250, 250, 250, 1 )",
        "ios": {
          "shadowOpacity": 0.25,
          "shadowOffset": {
            "y": 10
          }
        }
      }
    },
    "&-login": {
      "&-button": {
        "padding": 20
      }
    }
  },
  "#peoples": {
    "image": "peoples.png",
    "imageFillType": "ASPECTFIT",
    "height": 430,
    "visible": true,
    "marginTop": 20
  },
  "#flWrapperGetStarted": {
    "paddingRight": 40,
    "paddingLeft": 40,
    "top": -30
  },
  "#btnGetStarted": {
    "backgroundColor": {
      "normal": "rgba( 252, 250, 250, 1 )"
    },
    "textColor": "rgba( 255, 75, 58, 1 )",
    "ios": {
      "shadowRadius": null
    },
    "borderRadius": 32
  },
  "#txtViewTitle": {
    "font": {
      "name": "",
      "family": "",
      "style": "",
      "size": 64
    }
  },
  "#textViewTitle": {
    "textColor": "rgba( 255, 255, 255, 1 )",
    "font": {
      "name": "",
      "family": "SFProText",
      "style": "Semibold",
      "size": 64,
      "bold": true,
      "italic": false
    },
    "lineSpacing": 0,
    "height": 200
  },
  ".login": {
    "&-header": {
      "&-icon": {
        "image": "bella_onoje.png",
        "height": 100,
        "width": 100,
        "imageFillType": "ASPECTFIT"
      }
    }
  },
  "#logintext": {
    "width": 100,
    "textAlignment": "MIDCENTER",
    "height": 30
  },
  "#logintextWrapper": {
    "flexProps": {
      "alignSelf": "STRETCH",
      "alignContent": "CENTER",
      "alignItems": "CENTER"
    },
    "height": 30
  },
  "#pageLogin": {
    "backgroundColor": "rgba( 242, 242, 242, 1 )"
  },
  "#loginForm": {
    "height": 300,
    "padding": 30
  },
  "#loginTitleWrapper": {
    "height": 30,
    "width": 100
  },
  "#pgImageView": {
    "height": 100,
    "width": 100,
    "image": "bella_onoje.png",
    "imageFillType": "ASPECTFILL"
  },
  "#login": {
    "&-tiitle": {
      "&-wrapper": {
        "flexProps": {
          "alignItems": "CENTER",
          "alignContent": "CENTER",
          "justifyContent": "SPACE_BETWEEN"
        },
        "height": 300,
        "backgroundColor": "rgba( 255, 255, 255, 1 )",
        "borderRadius": 30,
        "ios": {
          "shadowOffset": {
            "y": 10
          }
        },
        "borderTopLeftRadius": -2,
        "borderTopRightRadius": -2
      }
    },
    "&-button": {
      "backgroundColor": {
        "normal": "rgba( 250, 74, 12, 1 )"
      },
      "borderRadius": 32
    }
  },
  "#loginTitle": {
    "width": 100,
    "height": 30,
    "textAlignment": "MIDCENTER",
    "textColor": "rgba( 0, 0, 0, 1 )",
    "borderWidth": null
  },
  "#userName": {
    "height": 80
  },
  "#forgotPassCode": {
    "backgroundColor": "rgba( 250, 12, 12, 0 )",
    "textColor": "rgba( 250, 74, 12, 1 )"
  }
}